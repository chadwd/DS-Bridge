{
  "name": "design-system",
  "version": "0.0.1",
  "description": "A white-label design system built with Vue 3, Vite, and Vuetify 3",
  "author": "Chad Dunbar",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/DS-Bridge.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "design-system",
    "component-library",
    "ui",
    "whitespace",
    "design-tokens"
  ],
  "homepage": "https://github.com/yourusername/DS-Bridge#readme",
  "bugs": {
    "url": "https://github.com/yourusername/DS-Bridge/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": "./dist/components/index.js",
    "./tokens": "./dist/tokens/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vuetify": "^3.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "material-design-icons-iconfont": "^6.7.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.10",
    "vitepress": "^1.0.0-rc.45",
    "vitest": "^1.0.4"
  }
}
